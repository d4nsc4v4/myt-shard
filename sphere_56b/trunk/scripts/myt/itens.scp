[ITEMDEF I_boleadera]
DEFNAME=I_boleadera
ID=I_Cannon_Ball
NAME=Boleadera
RESOURCES=i_hides_cut 50,i_cannon_ball
SKILLMAKE=TAILORING 60.0


ON=@CREATE
    color=colors_brown
    attr attr_decay|attr_can_decay


ON=@dclick
    IF !(<TOPOBJ.UID>==<SRC.UID>)
        SRC.SYSMESSAGE A boleadera deve estar em sua mochila!
    RETURN 1
    END IF
    IF (<TIMER> >=1)
        SRC.SYSMESSAGE Voce ainda nao tem forcas para tacar novamente!
    RETURN 1
    ENDIF
    IF (<SRC.RESTEST 2 i_bola>)
        src.sysmessage voce so pode possuir uma boleadera em sua mochila por vez!
    RETURN 1
    ENDIF
    if <src.STR> < 90
        src.sysmessage Voce nao e forte o suficiente para usar a boleadera!
    return 1
    else
        TARGET quem voce deseja desmontar?
    ENDIF
    RETURN 1


on=@targon_item
    src.sysmessage Voce nao pode usar isso em itens!
RETURN 1

on=@targon_ground
    src.sysmessage Voce acerta no solo!
return 1


on=@targon_char
    IF <SRC.TARG>=<src.uid>
        src.sysmessage Voce tenta usar a boleadera em voce mesmo ,mas , descobre que e impossivel!
    return 1
    elIF <SRC.TARG.NPC>
        src.sysmessage Voce nao pode usar isso em npcs!
    return 1
    elIF <SRC.TARG.ISGM>
        src.sysmessage Voce nao pode desmontar GMS!
    return 1
    elIF <src.TARG.DISTANCE> > 9 // 9 blocks away..
        src.sysmessage Voce esta muito longe do seu alvo!
    return 1
    else
    if (<src.targ.findlayer.25.uid> )
        SRC.TARG.DAMAGE 1 01 <SRC.UID>
        SRC.TARG.HITPOINTS=<SRC.TARG.HITPOINTS>+1
        src.anim 9
        src.act=<src.targ.findlayer.25.more2>
        src.act.p=<src.targ.p>
        src.targ.findlayer.25.remove
        src.targ.update
        src.update
        src.targ.emote Voce foi desmontado por <src.name>
        src.targ.ANIM 22
        REGION.ALLCLIENTS FIX
        update
        TIMER 20
    else
        src.sysmessage O alvo deve estar montado!
    endif
    return 1
    ENDIF












