[ITEMDEF i_suco_gummy]
NAME=suco de frutas Gummy
ID=0F00
TYPE=T_SCRIPT
WEIGHT=1

CATEGORY=MyT - Provisions - miscellaneous
SUBSECTION=Bebidas
DESCRIPTION=Suco de frutas Gummy

on=@dclick
src.emoteyellow tomando <name>
src.sfx 01E8
src.effect 3 i_fx_heal_effect 0 16 0 0916
src.tag.oldstam=<src.stam>
src.stam=500
src.hits=<src.maxhits>
src.mana=<src.maxmana>
src.timerf 60 f_NoSucoFrutasGummy
remove

[FUNCTION f_NoSucoFrutasGummy]
stam=<tag.oldstam>
tag.oldstam=
sysmessagered O efeito do Suco de Frutas Gummy passou.

