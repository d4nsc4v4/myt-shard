[ITEMDEF i_colmeia]
NAME=colmeia
type=t_script
ID=091a

CATEGORY=MyT - Items by Professions
SUBSECTION=Cook
DESCRIPTION=Colmeia

ON=@CLIENTTOOLTIP
f_sendTooltip Mel: <eval <more1>>,Cera: <eval <more2>>

ON=@create
more1=3
more2=2
timer = 13*60

ON=@TIMER
if (<more1>  <=5)
 more1=(<more1>+1)  // número de jarros de mel por vez.
 timer = 13*60
endif
if (<more2>  <=6)
 more2=(<more2>+1)  // número de ceras por vez.
 timer = 13*60
endif
update
return 1

ON=@DCLICK
src.newitem=i_enchame_abelhas
src.act.link=<src.uid>
src.act.p=<src.p>
if (<more1>>0)
 src.f_getHoney <UID>
ELSE
    src.sysmessageyellow Nao ha mel nesta colmeia.
ENDIF
if (<more2>>0)
    if (<src.findlayer.1.dispiddec>==3921) || (<src.findlayer.1.dispiddec>==02d21)
        more2 -= 1
        src.newitem i_beeswax
        src.new.bounce
    endif
ELSE
    src.sysmessageyellow Nao ha cera nesta colmeia.
endif
timer = 13*60
update
return 1

[FUNCTION f_getHoney]
obj=<argn>
forconttype t_recipient 2
 if ( (!<more2>) || (<more2>==LIQUID_HONEY) ) && (<more1> < <TDATA1>) && (!<LOCAL.done>)
  FillRecipient LIQUID_HONEY,2
  obj.more1 -= 1
  LOCAL.done=1
 endif
endfor

