 newnpc <LOCAL.creature>
 new.movenear <uid> 2
 new.effect 3 i_fx_sparkle 26 26 26
 new.sfx 0217
 new.timerf 120,remove
 obj=<new.uid>
 serv.newitem i_coin_silver
 new.amount=5
 new.cont=<obj>
endif
return 1

[ITEMDEF i_bola_cristal]
id=i_crystal_ball
type=t_script
name=bola de cristal

CATEGORY=MyT - Quest Items
SUBSECTION=Quest
DESCRIPTION=Bola de cristal

on=@create
more1=4

on=@dclick
if (<more1>)
 more1=(<more1> +(-1))
 if (0<src.tag.fRP> <= -3 )
  src.message Seu futuro eh um desastre total!!! (<more1> cargas).
 elif (0<src.tag.fRP> == -2 )
  src.message Seu futuro eh MUITO penoso (<more1> cargas).
 elif (0<src.tag.fRP> == -1 )
  src.message Seu futuro eh um tanto penoso (<more1> cargas).
 elif (0<src.tag.fRP>==0)
  src.message Seu futuro eh incerto (<more1> cargas).
 elif (0<src.tag.fRP>==1)
  src.message Seu futuro eh bom... (<more1> cargas).
 elif (0<src.tag.fRP>==2)
  src.message Seu futuro eh muito bom. (<more1> cargas).
 elif (0<src.tag.fRP>==3)
  src.message Seu futuro eh promissor! (<more1> cargas).
 elif (0<src.tag.fRP>==4)
  src.message Seu futuro eh brilhante! (<more1> cargas).
 elif (0<src.tag.fRP> >= 5)
  src.message Seu futuro eh ABSOLUTAMENTE GENIAL!!! (<more1> cargas).
 endif
 return 1
else
 dorand (10)
  src.message Voce ve uma mao segurando um machado.
  src.message Voce ve a sua infancia.
  src.message Voce ve muitros dragoes!
  src.message Voce ve um olho imenso.
  src.message Voce ve uma espada de cristal sendo quebrada.
  src.message Voce ve seu copo em chamas!!!
  src.message Voce ve um vulto correndo nas sombras.
  src.message Voce ve uma mulher com uma faca ensanguentada na mao.
  src.message Voce ve um cajado negro. 
  src.message Voce ve que esta bola ja nao funciona mais. Jogue-a fora.
 enddo
 return 1
endif

