[ITEMDEF i_suco_fantasma]
NAME=suco fantasma
ID=0EFB
TYPE=T_SCRIPT
WEIGHT=1

CATEGORY=MyT - Provisions - miscellaneous
SUBSECTION=Bebidas
DESCRIPTION=Suco fantasma

on=@dclick
src.emoteyellow tomando <name>
src.sfx 050
src.timerf 1 sysmessagered,O suco desapareceu!
timerf 1 remove
src.timerf 2 sfx,016
local.creature={c_spectre 1 c_shade 2 c_wailing_banshee 2}
src.timerf 15 f_conjure_creature,<local.creature>,120
return 1

[ITEMDEF i_suco_risada]
NAME=suco da risada
ID=0EFC
TYPE=T_SCRIPT
WEIGHT=1

CATEGORY=MyT - Provisions - miscellaneous
SUBSECTION=Bebidas
DESCRIPTION=Suco da risada

on=@dclick
src.emoteyellow tomando <name>
src.sfx 050
src.timerf 10 f_suco_risada,15
remove
return 1

[FUNCTION f_suco_risada]
if (0<argn>)
 ctag.suco_risada=<argn>
 timerf 1 f_suco_risada
else
 if (!<ctag0.suco_risada>)
  ctag0.suco_risada=
  say Ai ai... Suco da risada!
 else
  ctag0.suco_risada -= 1
  dorand 4
   rir 
   rir
   rir
   say Rairirairarara! Suco da risada!
   sfx <SEX 0431/0321>
  end
  timerf <R2,5> f_suco_risada
 endif
endif

[ITEMDEF i_suco_fedor]
NAME=suco do fedor
ID=0EFD
TYPE=T_SCRIPT
WEIGHT=1

CATEGORY=MyT - Provisions - miscellaneous
SUBSECTION=Bebidas
DESCRIPTION=Suco do fedor

on=@dclick
src.emoteyellow tomando <name>
src.sfx 050
src.timerf 5 f_suco_fedor
remove
return 1

[FUNCTION f_suco_fedor]
peidar
for i 4
 serv.newitem i_moscas
 new.dispid=i_fx_smoke
 new.more1=<R2,5>
 new.more2=150
 new.color=0110
 new.type t_veneno
 new.name fuafa
 new.movenear <UID> 2
 new.timerd <R8,12>
end
forclients 12
 sysmessagered Voce sente um fedor insuportavel!
end

