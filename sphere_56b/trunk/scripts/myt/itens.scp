[ITEMDEF i_carma_aventureiros]
id=i_rune_marker
type=t_script
name=O Carma Dos Aventureiros

CATEGORY=MyT - Quest Items
SUBSECTION=Quest
DESCRIPTION=Carma dos aventureiros

on=@dclick
//Ele pode receber pontos de alma?
if (<src.tag.pontosdealma> >= 5)
 src.sysmessagorange Hey! Voce esta profanando as leis de Umbra!
 src.tag.morte_desmaios=<DEF.max_desmaios>
 src.timerf 2,suicide
 remove
 return 1
endif

local.roll=<R1,11>
if (<local.roll> == <src.ctag0.rp_grade>)
 src.sysmessageyellow O item desaparece magicamente
elif (<local.roll> > <src.ctag0.rp_grade>)
 src.sysmessagered O carma nao esta a seu favor hoje.
 if (<src.ctag0.rp_grade> < 5)
  src.morte_decrementaPontos 1
 else
  LOCAL.creature=<eval {c_changeling 1 c_devourer_souls 1 c_gargoyle 1}>
  newnpc <LOCAL.creature>
  new.movenear <uid> 2
  new.effect 3 i_fx_sparkle 26 26 26
  new.sfx 0217
  new.timerf 120,remove
  obj=<new.uid>
  serv.newitem i_coin_silver
  new.amount=5
  new.cont=<obj>
 endif
else
 src.sysmessageblue Voce eh um bom jogador. O carma lhe deu 1 ponto de alma
 src.morte_incrementaPontos 1
endif
remove
return 1

