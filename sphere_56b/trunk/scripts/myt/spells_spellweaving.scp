  obj.str=<eval (<obj.str>*3>)/2>
  obj.dex=<eval (<obj.dex>*3>)/2>
  obj.aid
  obj.tactics 30.0
  obj.wrestling 40.0
  obj.color <R88,109>
  obj.name pardal de Belarin
 elif (!<R0,89>)
  obj.str=<eval <obj.str>*2>>
  obj.dex=<eval <obj.dex>*2>>
  obj.aid
  obj.tactics 50.0
  obj.wrestling 70.0
  obj.color 06E7
  obj.name Principe Pardal
 else
  obj.tactics 10.0
  obj.wrestling 15.0
  obj.name pardal
 endif
 obj.Update
 obj.attack <uid>
ENDFOR
