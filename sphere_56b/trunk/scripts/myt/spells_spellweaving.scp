[ITEMDEF i_scroll_sens_frio]
ID=0E37
NAME=Pergaminho sensibilidade ao frio
RESOURCES=i_carta,i_r_raiz_de_mandrake,i_r_nightshade
SKILLMAKE=INSCRIPTION 30.0, Spellweaving 20.0
TYPE=T_SCROLL
WEIGHT=1
//CIRCLE=1
