//*****************************************************************************
// i_magic_mark
//*****************************************************************************
[ITEMDEF i_magic_mark]
ID=i_coin_copper
NAME=marcador de magia
TYPE=t_script

ON=@CREATE
attr=attr_invis|attr_move_never|attr_decay
color=01

ON=@DCLICK
if <tag0.effect>==1) && <link.findlayer.layer_hand1>
    obj=<link.findlayer.layer_hand1>
    obj.p=<link.p>
    link.sysmessagered Ficou muito quente e queimou sua mao!!!
    link.emotered larga
    link.damage 5 dam_god <uid>
    tag.ocolor=<obj.color>
    tag.oname=<obj.name>
    obj.name=<obj.name> (quente)
    sfx 0227
    obj.attr |= attr_move_never
    obj.color=079d
    link=<obj>
    timer=<more1>
    obj.update
elif <tag.effect>==2
//??    link.acaoclima 3
    timer {3 5}
elif <tag.effect>==3
    link.sysmessagered Voce esta passando mal.
    link.emotegreen parece enjoado
    timer {5 10}
elif <tag.effect>==4
    link.ft_rain
    timer {1 3}
elif <tag.effect>==5
    timer {1 3}
elif <tag.effect>==6
    link.ft_snow
    timer {1 3}
endif
return 1

ON=@TIMER
if <tag.effect>==1
    link.attr &= ~attr_move_never
    link.color=<tag.ocolor>
    link.name=<tag.oname> 
    link.update
    remove
elif <more1>
    if <tag.effect>==2
    //?? link.acaoclima 2
        more1 -= 1
        timer {3 5}
    elif <tag.effect>==3
        link.emotered vomita um sapo
        link.newnpc 051
        link.newitem i_poca
        link.new.color 0106
        link.new.attr=012
        link.new.name=vomito
        link.new.timer {8 12}
        link.new.p=<link.p>
        link.new.update
        p=<link.p>
        more1 -= 1
        timer {30 120}
    elif tag.effect>==4
        serv.newitem i_m_raio
        new.p=<p>
        new.link=<uid>
        new.timerd=1
        new.attr=attr_move_never|attr_invis
        new.update
        more1 -= 1
        timerd {5 15}
    elif <tag.effect>==5
        serv.newitem i_m_meteoro
        new.p=<p>
        new.link=<uid>
        new.timerd=1
        new.attr=attr_move_never
        new.update
        more1 -= 1
        timerd {5 15}
    elif <tag.effect>==6
        serv.newitem i_m_gelo
        new.p=<p>
        new.link=<uid>
        new.timerd=1
        new.attr=attr_move_never
        new.update
        more1 -= 1
        timerd {5 15}
    else
        remove
    endif        
else
    remove
endif
return 1


