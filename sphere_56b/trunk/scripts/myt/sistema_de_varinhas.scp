//*****************************************************************************
// i_mry_castspl
//*****************************************************************************
//more2=tempo em decimos de segundo
[ITEMDEF i_mry_castspl]
ID=02087
TYPE=t_eq_script
NAME=Invocando Magia
LAYER=layer_special

ON=@DCLICK
LOCAL.mana=<cont.focus_mana <eval <tag.mana>>>
if (<LOCAL.mana> > <cont.mana>)
    cont.sysmessagered Voce nao tem mana o suficiente.
    remove
    return 1
endif 
cont.mana -= <LOCAL.mana>
more2=<cont.focus_time <eval <tag.delay>>>

cont.anim <tag.anim>
sfx <tag.snd1>

cont.freeze
cont.sysmessageyellow Preparando a magia...
target

timerd 1
return 1

ON=@TIMER
if <more1>==1
    cont.unfreeze
    remove
    return 1
//loop pre-cast
elif <more2>
    more2 -= 1
    timerd 1
    return 1
endif

cont.unfreeze

if !<cont.focus_tryUse <tag0.dif>>
    dorand 5
        sfx 05c
        sfx 05d
        sfx 05b
        sfx 061
        sfx 0227
    enddo
    serv.newitem i_fx_smoke_small
    new.p=<cont.p>
    new.timer 2
    new.attr 012
    new.move 0 0 10
    cont.sysmessagered A magia nao funcionou.
    cont.skill_gain skill_magery
    remove
    return 1
else
    cont.sysmessageblue Selecione o alvo:
    timer 30
    more1 1
    sfx <tag.snd2>
endif

return 1

ON=@TARGON_CHAR
if <more2>
    cont.sysmessagered A magia não esta pronta.
    cont.sysmessageyellow Espere...
    target
    return 1
endif

LOCAL.flags=<tag0.spellflags>

if !<src.targ.canseelos>
    cont.sysmessagered Voce nao consegue ver o alvo.
    cont.sysmessageblue Selecione o alvo:
    target
    return 1
elif <tag0.mindist>
    LOCAL.dist=<src.distance <src.targ>>
    if (<LOCAL.dist> > <tag0.mindist>)
        cont.sysmessagered Alvo muito distante
        cont.sysmessageblue Selecione o alvo:
        target
        return 1 
    endif
endif

if !<tag.timer>
    tag.timer=<src.focus_time <src.magery>>
endif

LOCAL.dano=<src.focus_mana <tag.dam>>
LOCAL.timer=<cont.focus_time <tag.timer>>

if (<LOCAL.flags>&spl_resist) && (<src.targ>!=<src>)
    if (<src.targ.skillcheck MagicResistance <eval <cont.focus_skill>/10>>)
        src.targ.sysmessageblue Voce faz um esforco para resistir a magia.
        src.targ.emotered resistiu
        local.resist=<eval <src.targ.magicresistance>/10>
        local.dano=<eval <local.dano-(((local.dano*local.resist)/100)-((30*local.dano)/100))>+{1 5}>
        LOCAL.timer=<eval <local.timer-(((local.timer*local.resist)/100)-((30*local.timer)/100))>+{5 10}>
    elif (<src.targ.NPC>==0)
        src.targ.GAIN_SKILL magicresistance
    endif
endif

cont.anim <tag.anim>

if <LOCAL.flags>&spl_harm
    src.targ.attack <src.uid>
endif

if <tag0.fx_t>
    src.targ.effect 3 <tag.fx_t> 0 32 0
endif

if <LOCAL.flags>&spl_mry
    serv.newitem i_mry_spl
    new.cont=<src.targ.uid>
    new.more1=<LOCAL.timer>
    new.timer 1
    if <tag0.str>
        src.targ.str += <tag.str>
        new.tag.str=1
    endif
    if <tag0.dex>
        src.targ.dex += <tag.dex>
        new.tag.dex=1
    endif
    if <tag0.int>
        src.targ.int += <tag.int>
        new.tag.int=1
    endif
    if <tag0.hp>
        src.targ.hits += <tag.hp>
        if <src.targ.hits> > <src.targ.maxhits>
            src.targ.hits=<src.targ.maxhits>
        endif
    endif
    if <tag0.body>
        src.targ.body=<tag.body>
        new.tag.obody=1
        src.targ.update
    endif
    if !(strmatch(<tag.msay>,)) && !<src.targ.NPC>
        new.tag.msay <tag.msay>
    endif
    if (0<tag.t_flag>)
        src.targ.flags |= <tag.t_flag>
        new.tag.t_flag=<tag.t_flag>
        src.targ.update
    endif
    new.tag.fx_t=<tag.fx_t>
    new.tag.snd1=0<tag.snd1>
endif

if <tag0.mmark>
    serv.newitem i_magic_mark
    new.p=<src.p>
    new.link=<src.targ>
    new.more1=<LOCAL.timer>
    new.tag.effect=<tag.mmark>
    new.use
endif

if <tag0.spelleffect>
    src.targ.spelleffect <tag.spelleffect> <eval 0<src.focus_skill>>
endif

if (0<local.flags>&spl_missil)
    if !<tag.damt>
        tag.damt=04
    endif
    src.targ.damage <LOCAL.dano> <tag.damt> <src>
    if (<src.isgm>)
        src.targ.say <eval <LOCAL.dano>>
    endif
    if <tag0.repeat> > 1
        tag.repeat -= 1
        more1=0
        tag.delay= <eval <tag0.delay>/2>
        obj=<src.targ>
        src.findid.i_mry_castspl.use
        obj.effect 0 <tag.fx> 16 0 0
        obj.effect 3 <tag.fx_t> 0 9 0
        return 1
    endif
    src.targ.effect 0 <tag.fx> 16 0 0
    src.targ.effect 3 <tag.fx_t> 0 9 0
endif

remove
return 1

ON=@TARGON_GROUND
cont.sysmessagered Alvo invalido...
cont.sysmessageblue Selecione o alvo:
target
return 1

on=@targon_item
cont.sysmessagered Alvo invalido...
cont.sysmessageblue Selecione o alvo:
target
return 1

ON=@TARGON_CANCEL
cont.sysmessagered Cancelado.
cont.mana += <eval <tag.mana>/3>
cont.unfreeze
remove
return 1

