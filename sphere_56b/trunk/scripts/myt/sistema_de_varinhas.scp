//*****************************************************************************
// t_focus
//*****************************************************************************
[TYPEDEF t_focus]

ON=@EQUIP
SRC.DSPEECH +spk_mage

ON=@UNEQUIP
SRC.DSPEECH -spk_mage

ON=@DCLICK
equip
src.sysmessageyellow Deve recitar as palavras magicas para usar...
return 1

//*****************************************************************************
//*****************************************************************************
// FUNCTIONs
//*****************************************************************************
//*****************************************************************************

//*****************************************************************************
// focus_skill
//*****************************************************************************
[FUNCTION focus_skill]
local.int=<eval <int>>*10
local.mag=<eval <magery>>*8
local.mod=(<tag.mag>-5)*120
local.ret=(<local.int>+<local.mag>-<local.mod>)/9
return <eval <local.ret>>

//*****************************************************************************
// focus_mana
//*****************************************************************************
[FUNCTION focus_mana]
local.mana=<argn>
local.mag=<tag.mag>-5
local.mod=(<local.mana>*<local.mag>)/15
local.ret=<local.mana>+<local.mod>
return <eval <local.ret>>

//*****************************************************************************
// focus_time
//*****************************************************************************
[FUNCTION focus_time]
local.time=<argn>
local.mag=<tag.mag>-5
local.mod=(<local.time>*<local.mag>)/10
local.ret=<local.time>+<local.mod>
return <eval <local.ret>>

//*****************************************************************************
// focus_tryUse
//*****************************************************************************
[FUNCTION focus_tryUse]

if <isgm>
    return 1
endif

LOCAL.dif=<skillchance <focus_skill>,<argn>>

//se magia tiver dominio, ve bonus/penalidades
if !(strmatch(<findid.i_mry_castspl.tag.splt>,)
    LOCAL.splt=<findid.i_mry_castspl.tag.splt>
    LOCAL.spcnt=0
    
    if <LOCAL.splt>==splt_vida
        LOCAL.spcnt=<tag0.sfe_vida>
    elif <LOCAL.splt>==splt_agua
        LOCAL.spcnt=<tag0.sfe_agua>
    elif <LOCAL.splt>==splt_materia
        LOCAL.spcnt=<tag0.sfe_materia>
    elif <LOCAL.splt>==splt_ar
        LOCAL.spcnt=<tag0.sfe_ar>
    elif <LOCAL.splt>==splt_fogo
        LOCAL.spcnt=<tag0.sfe_fogo>
    elif <LOCAL.splt>==splt_morte
        LOCAL.spcnt=<tag0.sfe_morte>
    elif <LOCAL.splt>==splt_terra
        LOCAL.spcnt=<tag0.sfe_terra>
    elif <LOCAL.splt>==splt_energia
        LOCAL.spcnt=<tag0.sfe_energia>    
    endif
    
    //se usuario nao tiver pontos nesse dominio, aplica penalidade 25%
    if !<LOCAL.spcnt>
        LOCAL.dif -= 250
    //aplica 5% de bonus por ponto no dominio        
    else
        LOCAL.dif += <eval 50*<LOCAL.spcnt>>
    endif
    
endif


if <R1000> < <LOCAL.dif>
    return 1
endif

return 0

//*****************************************************************************
// effecter
//*****************************************************************************
[FUNCTION effecter]
serv.newitem i_effecter
new.link <src>
new.p=<src.p>
new.timerd 1

//*****************************************************************************
// msay
//*****************************************************************************
[FUNCTION msay]
if (strmatch(<args>,louco)
    sysmessagered Voce esta completamente louco!
    dorand 21
        say Eu vim pelo portal dos deuses do passado!
        emotered morde os bracos
        say Eu sou Atila, o Ardente! Meu pubis esta em chamas!
        say Mamaaaaaae! Cospiram em mim!
        say Cadela, vadia... humpf!
        say Posso segurar seu olho?
        say MWAHUAHUAHUAHUAHUAHUAHUAAAAAaaaaaaa!!!
        say Bilurilis? Onde esta voce, Bilurilis?
        emotered defecando
        say O homem de burro comeu a lingua! Eu vi sim!
        say A palavra dele e falsa! Tao falsa quanto o meu cajado!
        emotegreen apalpa as nadegas
        say Nao fui eu! Pergunte a Mond!
        say Como voce pode fumar isto?
        say Nao me toque! Estou purificado pelo nome de... MIM! HAHAHA!
        say *sussura* Coeliiiiiiinho, vem, vem, veeeeem.. pspssssss.
        emotegreen vomita sangue
        say Nao conte com isso...
        emotered arregala os olhos
        say Hummmm!!! Que delicia de perninha!
        emotered abocanha
    enddo
elif (strmatch(<args>,sentidos)
    DORAND 11
        sysmessagegreen Voce sente seus tentaculos alongarem ate a lua.
        sysmessagegreen Voce esta pisando nos seus olhos!
        sysmessageblue Nem mesmo um dragao pode deter sua furia!
        sysmessagegreen Os duendes da gelatina estao pinicando seua barbatana.
        sysmessageblue Voce esta invisivel. Ninguem pode te ver.
        sysmessagered Voce esta cozinhando num enorme caldeirao!
        sysmessageyellow Aquele sujeito beliscou sua bunda.
        sysmessageyellow Seu amigo Salcichao esta do seu lado.
        sysmessagegreen Voce ve o homem de burro comer a lingua. Ve sim.
        sysmessage Bilurilis acaba de fugir.
        sysmessageblue Voce esta purificado em nome de <tag.name>!
    ENDDO
endif

