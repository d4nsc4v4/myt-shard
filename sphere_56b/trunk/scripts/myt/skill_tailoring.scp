//*****************************************************************************
// t_wool - lã pura, use na roca para fazer novelos
//*****************************************************************************
[TYPEDEF t_wool]
ON=@DCLICK
if (<topobj>!=<src.uid>)
    src.sysmessagered A la precisa estar na sua mala
else
    if (<amount>>5)
        LOCAL.amt=5
    else
        LOCAL.amt=<amount>
    endif
    obj=<src.findid.i_faz_item.uid>
    obj.cleartags
    obj.tag.needtype=t_roca
    obj.tag.need=roca
    obj.tag.uid1=<uid>
    obj.tag.amt1=<LOCAL.amt>
    obj.tag.nam1=<name>
    obj.tag.skill=tailoring
    obj.tag.dif=0
    obj.tag.emote=fiando
    obj.tag.delay=<eval (18-(<src.tailoring>/66))*<LOCAL.amt>> //3 a 18 por lã
    obj.tag.item=i_yarn_ball
    obj.tag.amt=<LOCAL.amt>
    obj.tag.color=<color>
    obj.tag.f_init=iniciaRoca
    obj.tag.f_step=animaRoca
    obj.tag.f_end=paraRoca  
    src.fazitem
endif
return 1

