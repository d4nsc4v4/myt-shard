    obj.tag.item=i_barbante
    obj.tag.color=<color>
    obj.tag.f_init=iniciaRoca <src.targ.uid>,
    obj.tag.f_step=animaRoca <src.targ.uid>,
    obj.tag.f_end=paraRoca <src.targ.uid>,
    src.fazitem
    return 1 
endif
